<!DOCTYPE html>
<html>
<head>
  <title><%=  title %></title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div class="container" >

    <div id="header" class="span-15"> <h1> Link to logo here </h1> </div>
    <nav class="span-4 last round">
        <ul>
          <li><%= link_to "Home", '#' %> </li>
          <li><%= link_to "Help", '#' %> </li>
          <li><%= link_to "Sign In", '#' %> </li>
        </ul>
    </nav>

    <section class="span-24 last" <%= yield %> </section>

    <% if content_for?(:debug) %>
      <section class="span-15"> Test: <%= yield :debug %> </section>
    <% end %>

  </div>

</body>
</html>
