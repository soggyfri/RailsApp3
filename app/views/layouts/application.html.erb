<!DOCTYPE html>
<html>
<head>
  <title><%=  title %></title>
  <%= stylesheet_link_tag :all %>
  <%= stylesheet_link_tag 'blueprint/screen', :media => 'screen'  %>
  <%= stylesheet_link_tag 'blueprint/print',  :media => 'print' %>
  <%= stylesheet_link_tag 'custom', :media => 'screen' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div class="container" >

    <div id="header" class="span-15"> <h1> Link to logo here </h1> </div>
    <nav class="span-9 last round">
        <ul>
          <li><%= link_to "Home", '#' %> </li>
          <li><%= link_to "Help", '#' %> </li>
          <li><%= link_to "Sign In", '#' %> </li>
        </ul>
    </nav>

    <section class="span-10" <%= yield %> </section>

  </div>

</body>
</html>
